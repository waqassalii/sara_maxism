                                                </td>
                                                <!--                                                <td style="border-top: 1px solid black !important;">-->
                                                <!--                                                    <span t-field="o.partner_id.gender_type"/>-->
                                                <!--                                                    <span t-field="o.partner_id.name"/>-->
                                                <!--                                                    <br/>-->
                                                <!--                                                    <span t-field="o.partner_id.street"/>,-->
                                                <!--                                                    <span t-field="o.partner_id.city"/>-->
                                                <!--                                                </td>-->
                                            </tr>
                                        </table>
                                    </div>

                                    <div class="col-4" style="font-size: 14px;line-height: 1.3;">
                                        <table class="table table-sm">
                                            <tr>
                                                <thead class="main_border"
                                                       style="border: 1px solid black;background-color: lightgray;">
                                                    <th name="th_to" style="border-top: 1px solid black !important;">
                                                        <strong>Invoice To</strong>
                                                    </th>
                                                </thead>
                                                <td style="border-top: 1px solid black !important;height: 95px;">
                                                    <!--                                            <t t-if="o.dest_address_id">-->
                                                    <span t-field="o.invoice_to"
                                                          t-options="{&quot;widget&quot;: &quot;contact&quot;, &quot;fields&quot;: [&quot;address&quot;, &quot;name&quot;], &quot;no_marker&quot;: True}"
                                                          name="purchase_shipping_address"/>
                                                    <!--                                            </t>-->
                                                </td>
                                                <!--                                                <td style="border-top: 1px solid black !important;">-->
                                                <!--                                                    <span t-field="o.company_id.name"/>-->
                                                <!--                                                    <br/>-->
                                                <!--                                                    <span t-field="o.company_id.street"/>-->
                                                <!--                                                    <br/>-->

                                                <!--                                                    <span t-field="o.company_id.city"/>-->
                                                <!--                                                    <span t-field="o.company_id.zip"/>-->
                                                <!--                                                </td>-->
                                            </tr>
                                        </table>
                                    </div>

                                    <div class="col-4" style="font-size: 14px;line-height: 1.3;">
                                        <table class="table table-sm">
                                            <tr>
                                                <thead class="main_border"
                                                       style="border: 1px solid black;background-color: lightgray;">
                                                    <th name="th_to" style="border-top: 1px solid black !important;">
                                                        <strong>Ship To</strong>
                                                    </th>
                                                </thead>
                                                <td style="border-top: 1px solid black !important;height: 95px;">
                                                    <!--                                            <t t-if="o.dest_address_id">-->
                                                    <span t-field="o.ship_to"
                                                          t-options="{&quot;widget&quot;: &quot;contact&quot;, &quot;fields&quot;: [&quot;address&quot;, &quot;name&quot;], &quot;no_marker&quot;: True}"
                                                          name="purchase_shipping_address"/>
                                                    <!--                                            </t>-->
                                                </td>
                                            </tr>
                                        </table>
                                    </div>

                                </div>

                                <div class="oe_structure"/>
                                <div class="row" style="font-size: 14px;">
                                    <div class="col-9">
                                        <br/>
                                        <h2 t-if="o.state == 'draft'">Purchase Order #
                                            <span t-field="o.name"/>
                                        </h2>
                                        <h2 t-if="o.state in ['sent', 'to approve']">Purchase Order #
                                            <span t-field="o.name"/>
                                        </h2>
                                        <h2 t-if="o.state in ['purchase', 'done']">Purchase Order #
                                            <span t-field="o.name"/>
                                        </h2>
                                        <h2 t-if="o.state == 'cancel'">Cancelled Purchase Order #
                                            <span t-field="o.name"/>
                                        </h2>
                                    </div>
                                    <div class="col-3" style="font-size: 14px;line-height: 1.5;margin-left: 50px;">
                                        <strong>NTN :</strong>
                                        <span t-field="o.company_id.vat"/>
                                        <br/>
                                        <strong>STRN :</strong>
                                        <span t-field="o.company_id.company_strn"/>
                                        <br/>
                                        <strong>DRAP LIC :</strong>
                                        <span>ELI-0052</span>
                                        <br/>
                                        <strong>QF Code :</strong>
                                        <span t-field="o.company_id.purchase_qf_code"/>
                                    </div>
                                </div>
                                <!--  Table 1 -->
                                <table class="table table-sm o_main_table" style="font-size: 14px;line-height: 1.3;">
                                    <thead class="main_border"
                                           style="border: 1px solid black;background-color: lightgray;">
                                        <tr>
                                            <th name="th_ref" class="text-center"
                                                style="border-top: 1px solid black;border-right: 1px solid black;">
                                                <strong>Your Ref.</strong>
                                            </th>
                                            <th name="th_tref" class="text-center"
                                                style="border-top: 1px solid black;border-right: 1px solid black;">
                                                <strong>Tracking Ref.</strong>
                                            </th>
                                            <th name="th_po" class="text-center"
                                                style="border-top: 1px solid black;border-right: 1px solid black;">
                                                <strong>PO Date</strong>
                                            </th>
                                            <th name="th_shipping_date" class="text-center"
                                                style="border-top: 1px solid black;border-right: 1px solid black;">
                                                <strong>Shipping Date</strong>
                                            </th>
                                            <th name="th_payment" class="text-center"
                                                style="border-top: 1px solid black;border-right: 1px solid black;">
                                                <strong>Payment Terms</strong>
                                            </th>
                                        </tr>
                                    </thead>

                                    <tbody style="text-align: center;">
                                        <t t-foreach="docs" t-as="o">
                                            <tr>
                                                <td style="border-top: 1px solid black;">
                                                    <span t-esc="o.partner_ref"/>
                                                </td>
                                                <td style="border-top: 1px solid black;">
                                                    <span t-esc="o.tracking_ref"/>
                                                </td>
                                                <td style="border-top: 1px solid black;">
                                                    <t t-if="o.date_order">
                                                        <span t-esc="o.date_order.strftime('%d-%b-%Y')"/>
                                                    </t>
                                                    <t t-else="o.date_approve">
                                                        <span t-esc="o.date_approve.strftime('%d-%b-%Y')"/>

                                                    </t>
                                                </td>
                                                <td class="text-center" style="border-top: 1px solid black;">
                                                    <span t-esc="o.date_planned.strftime('%d-%b-%Y')"/>
                                                </td>
                                                <td class="text-center" style="border-top: 1px solid black;">
                                                    <span t-field="o.payment_term_id.name"/>
                                                </td>
                                            </tr>
                                        </t>
                                    </tbody>
                                </table>
                                <!--  Table-2 -->
                                <table class="table table-sm o_main_table" style="font-size: 14px;line-height: 1.3;">
                                    <thead class="main_border"
                                           style="border: 1px solid black;background-color: lightgray;">
                                        <tr>
                                            <th name="th_vendor" class="text-center"
                                                style="border-top: 1px solid black;border-right: 1px solid black;">
                                                <strong>Vendor Contact</strong>
                                            </th>
                                            <th name="th_vtel" class="text-center"
                                                style="border-top: 1px solid black;border-right: 1px solid black;">
                                                <strong>Vendor Tel.</strong>
                                            </th>
                                            <th name="th_email" class="text-center"
                                                style="border-top: 1px solid black;border-right: 1px solid black;">
                                                <strong>Vendor E-mail</strong>
                                            </th>
                                            <th name="th_currency" class="text-center"
                                                style="border-top: 1px solid black;border-right: 1px solid black;">
                                                <strong>Currency</strong>
                                            </th>
                                            <th name="th_incoterms" class="text-center"
                                                style="border-top: 1px solid black;border-right: 1px solid black;">
                                                <strong>Incoterms</strong>
                                            </th>
                                        </tr>
                                    </thead>

                                    <tbody style="text-align: center;">
                                        <t t-foreach="docs" t-as="o">
                                            <tr>
                                                <td style="border-top: 1px solid black;">
                                                    <span t-esc="o.partner_id.name"/>
                                                </td>
                                                <td style="border-top: 1px solid black;">
                                                    <span t-esc="o.partner_id.phone"/>
                                                </td>
                                                <td style="border-top: 1px solid black;">
                                                    <span t-esc="o.partner_id.email"/>
                                                </td>
                                                <td class="text-center" style="border-top: 1px solid black;">
                                                    <span t-field="o.currency_id.name"/>
                                                </td>
                                                <td class="text-center" style="border-top: 1px solid black;">
                                                    <span t-field="o.incoterm_id"/>
                                                </td>
                                            </tr>
                                        </t>
                                    </tbody>
                                </table>


                                <!--                                <div class="row zero_min_height">-->
                                <!--                                    <div class="col-12">-->
                                <!--                                        <div style="border-bottom: 1px solid black;"/>-->
                                <!--                                    </div>-->
                                <!--                                </div>-->
                                <!--                                <div class="row">-->
                                <!--                                    <div class="col-6" name="company_address">-->
                                <!--                                        <div t-field="o.company_id.partner_id"-->
                                <!--                                             t-options='{"widget": "contact", "fields": ["address", "name"], "no_marker": true}'-->
                                <!--                                        />-->
                                <!--                                    </div>-->
                                <!--                                </div>-->
                            </div>


                            <!--  Table 3 -->
                            <t t-set="total_qty" t-value="0"/>
                            <table class="table table-sm" style="font-size: 14px;">
                                <thead class="main_border">
                                    <tr>
                                        <th name="th_sn" class="text-center">
                                            <strong>S/N</strong>
                                        </th>
                                        <th name="th_code" class="text-center">
                                            <strong>Code</strong>
                                        </th>
                                        <th name="th_description" class="text-center">
                                            <strong>Description</strong>
                                        </th>
                                        <!--                                    <th name="th_taxes">-->
                                        <!--                                        <strong>Taxes</strong>-->
                                        <!--                                    </th>-->
                                        <!--                                    <th name="th_date_req" class="text-center">-->
                                        <!--                                        <strong>Date Req.</strong>-->
                                        <!--                                    </th>-->
                                        <th name="th_quantity" class="text-center">
                                            <strong>Qty</strong>
                                        </th>
                                        <th style="width: 16%;" name="th_price_unit" class="text-right">
                                            <strong>Unit Price</strong>
                                        </th>
                                        <th style="width: 16%;" name="th_amount" class="text-center">
                                            <strong>Total Price</strong>
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <t t-set="current_subtotal" t-value="0"/>
                                    <t t-foreach="o.order_line" t-as="line">
                                        <t t-set="current_subtotal" t-value="current_subtotal + line.price_subtotal"
                                           groups="account.group_show_line_subtotals_tax_excluded"/>
                                        <t t-set="current_subtotal" t-value="current_subtotal + line.price_total"
                                           groups="account.group_show_line_subtotals_tax_included"/>

                                        <tr t-att-class="'o_line_section' if line.display_type == 'line_section' else 'font-italic o_line_note' if line.display_type == 'line_note' else ''">
                                            <t t-if="not line.display_type">
                                                <td class="text-center">
                                                    <span t-esc="line_index+1"/>
                                                </td>
                                                <td class="text-center">
                                                    <span t-esc="line.product_id.default_code"/>
                                                </td>
                                                <td id="product" class="text-left">
                                                    <span t-field="line.product_id.name"/>
                                                </td>

                                                <!--                                            <td name="td_taxes">-->
                                                <!--                                                <span t-esc="', '.join(map(lambda x: x.name, line.taxes_id))"/>-->
                                                <!--                                            </td>-->
                                                <!--                                            <td class="text-center">-->
                                                <!--                                                <span t-field="line.date_planned"/>-->
                                                <!--                                            </td>-->
                                                <td class="text-center">
                                                    <span t-esc="'%.0f'% line.product_qty"/>
                                                    <t t-set="total_qty" t-value="total_qty+line.product_qty"/>
                                                    <!--                                                <span t-field="line.product_uom.name" groups="uom.group_uom"/>-->
                                                </td>
                                                <td class="text-right">
                                                    <span t-field="line.price_unit"/>
                                                </td>
                                                <td class="text-right">
                                                    <span t-field="line.price_subtotal"
                                                          t-options='{"widget": "float", "precision": 2}'/>
                                                </td>

                                            </t>
                                            <t t-if="line.display_type == 'line_section'">
                                                <td colspan="99" id="section">
                                                    <span t-field="line.name"/>
                                                </td>
                                                <t t-set="current_section" t-value="line"/>
                                                <t t-set="current_subtotal" t-value="0"/>
                                            </t>
                                            <t t-if="line.display_type == 'line_note'">
                                                <td colspan="99" id="note">
                                                    <span t-field="line.name"/>
                                                </td>
                                            </t>
                                        </tr>

                                        <t t-if="current_section and (line_last or o.order_line[line_index+1].display_type == 'line_section')">
                                            <tr class="is-subtotal text-right">
                                                <td colspan="99" id="subtotal">
                                                    <strong class="mr16">Subtotal</strong>
                                                    <span t-esc="current_subtotal"
                                                          t-options='{"widget": "float", "precision": 2}'/>
                                                </td>
                                            </tr>
                                        </t>
                                    </t>
                                    <tr>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td style="font-weight: bold;page-break-inside: always;" class="text-center">
                                            <div style="margin-left: -70px;">
                                                Total Qty:
                                                <span
                                                        t-esc="'%.0f'% total_qty"/>
                                            </div>

                                        </td>
                                        <td></td>
                                        <td></td>
                                    </tr>

                                </tbody>
                            </table>

                            <div id="total" class="row justify-content-end">
                                <div class="col-6">
                                    <table class="table table-sm" style="line-height: 1.1rem;">
                                        <tr>
                                            <thead class="main_border">
                                                <th name="th_to">
                                                    <strong>Special Instructions:</strong>
                                                </th>
                                            </thead>
                                            <td style="border: 1px solid black;">
                                                <p style="margin-bottom: 0px;" t-field="o.notes"/>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                                <div class="col-1">
                                </div>
                                <div class="col-5" style="line-height: 1.2rem;">
                                    <table>
                                        <tr>
                                            <td class="hide-border text-right" name="td_subtotal_label"
                                                style="border-top: 1px solid white !important">
                                                <strong>Sub total</strong>
                                            </td>
                                            <td class="text-right hide-border"
                                                style="border-top: 1px solid white !important">
                                                <span t-field="o.amount_untaxed"
                                                      t-options='{"widget": "float", "precision": 2}'/>
                                            </td>
                                        </tr>

                                        <tr>
                                            <td class="hide-border text-right"
                                                style="border-top: 1px solid white !important" name="td_taxes_label">
                                                Discount
                                            </td>
                                            <td class="text-right hide-border"
                                                style="border-top: 1px solid white !important">
                                                <span t-field="o.discount"
                                                      t-options='{"widget": "float", "precision": 2}'/>
                                            </td>


                                            <!--                                        <td name="td_taxes_label">Shipping &quot; Handling Charges:</td>-->
                                            <!--                                        <td class="text-right">-->
                                            <!--                                            <span t-field="o.amount_untaxed"-->
                                            <!--                                                  t-options="{&quot;widget&quot;: &quot;monetary&quot;, &quot;display_currency&quot;: o.currency_id}"/>-->
                                            <!--                                        </td>-->

                                        </tr>

                                        <tr>
                                            <td class="hide-border text-right"
                                                style="border-top: 1px solid white !important" name="td_taxes_label">
                                                Shipping &amp;
                                                Handling Charges:
                                            </td>
                                            <td class="text-right hide-border"
                                                style="border-top: 1px solid white !important">
                                                <!--                                                <span t-field="o.invoice_ids.advance_payment" t-options='{"widget": "float", "precision": 2}'/>-->
                                                <span t-field="o.shipping_cost"
                                                      t-options='{"widget": "float", "precision": 2}'/>
                                            </td>
                                        </tr>

                                        <tr>
                                            <td class="hide-border text-right"
                                                style="border-top: 1px solid white !important" name="td_taxes_label">
                                                Advance
                                                Payment/Adjustment:
                                            </td>
                                            <td class="text-right hide-border"
                                                style="border-top: 1px solid white !important">
                                                <span t-field="o.amount_tax"
                                                      t-options='{"widget": "float", "precision": 2}'/>
                                            </td>
                                        </tr>

                                        <tr class="o_total">
                                            <td class="text-right" style="border-top: 2px solid white !important;"
                                                name="td_amount_total_label">
                                                <strong>Net Total</strong>
                                            </td>
                                            <td style="border-bottom: 1px solid black !important;" class="text-right">
                                                <span t-field="o.net_total"
                                                      t-options='{"widget": "float", "precision": 2}'/>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>

