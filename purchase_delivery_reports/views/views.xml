<odoo>
    <data>


        <record model="ir.ui.view" id="stock_picking_view_custom_ext">
            <field name="name">stock.picking.view.custom.ext</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='picking_type_id']" position="after">
                    <field name="end_user"/>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="sale_order_view_custom_ext">
            <field name="name">sale_order_view_custom_ext</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='user_id']" position="after">
                    <field name="manufacturer"/>
                </xpath>
            </field>
        </record>

                <record model="ir.ui.view" id="sale_order_tree_view_custom_ext">
            <field name="name">sale_order_tree_view_custom_ext</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_quotation_tree_with_onboarding"/>
            <field name="arch" type="xml">
                <xpath expr="//tree/field[@name='amount_total']" position="after">
                                        <field name="net_total" sum="Net Total" widget="monetary" optional="show" decoration-bf="1"/>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="purchase_order_view_custom_ext">
            <field name="name">purchase_order_view_custom_ext</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_ref']" position="after">
                    <field name="ship_to"/>
                    <field name="invoice_to"/>
                </xpath>

                <xpath expr="//field[@name='order_line']/tree/field[@name='product_id']" position="after">
                    <field name="default_code"/>
                </xpath>

            </field>
        </record>

              <record model="ir.ui.view" id="purchase_order_tree_view_custom_ext">
            <field name="name">purchase_order_tree_view_custom_ext</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_kpis_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//tree/field[@name='amount_total']" position="after">
                                        <field name="net_total" sum="Net Total" widget="monetary" optional="show" decoration-bf="1"/>
                </xpath>
            </field>
        </record>



        <record model="ir.ui.view" id="partner_view_custom_ext">
            <field name="name">partner_view_custom_ext</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='fax_number']" position="before">
<!--                    <field name="gender_type"/>-->
                    <field name="account_no"/>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="company_view_custom_ext">
            <field name="name">company_view_custom_ext</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='vat']" position="after">
                    <field name="company_strn"/>
                </xpath>

                <xpath expr="//field[@name='website']" position="after">
                    <separator string="Qf-Codes"/>
                    <br/>

                    <field name="sale_qf_code"/>
                    <field name="invoice_qf_code"/>
                    <field name="Delivery_qf_code"/>
                    <field name="purchase_qf_code"/>

                    <separator string="Report Custom Logos"/>
                    <br/>

                    <field name="logo_sale"/>
                    <field name="logo_purchase"/>
                    <field name="logo_invoice"/>
                    <field name="logo_Delivery"/>
                    <separator string="Invoice Texts"/>
                    <!--                    class="oe_inline" nolabel="1"-->
                    <field name="invoice_text"/>
                    <field name="invoice_text_2"/>

                </xpath>
            </field>
        </record>


